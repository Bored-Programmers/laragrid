{
  "name": "bored-programmers/laragrid",
  "description": "Laravel grid package",
  "type": "library",
  "license": "MIT",
  "require": {
    "illuminate/support": "^10.0",
    "livewire/livewire": "^3.0",
    "kirschbaum-development/eloquent-power-joins": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "BoredProgrammers\\LaraGrid\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Matěj Černý",
      "email": "mat.cerny3@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "BoredProgrammers\\LaraGrid\\Providers\\LaraGridServiceProvider"
      ]
    }
  }
}
